***//app.component.ts//***

import { Component } from '@angular/core';

@Component({
  selector: 'app-root',
  templateUrl: './app.component.html',
  styleUrls: ['./app.component.css']
})
export class AppComponent {
  title = 'project2';

  cities:string[] = ["A", "B", "C", "D", "E", "F"];
}


***//app.component.html//***
<div>
  <h4>ngFor</h4>

  <ul>
    <li *ngFor="let city of cities">{{city}}</li>
  </ul>
</div>
